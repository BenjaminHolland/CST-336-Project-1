Villian
-
VillianId PK int
Name string
Address string

Order
--
OrderId PK int
VillianIdId int FK >- Villian.VillianId
OrderStatusId int FK >- os.OrderStatusID

OrderLine as ol
----
OrderLineId PK int
OrderId int FK >- Order.OrderId
HenchpersonId int FK >- h.HenchpersonId

Henchperson as h
------------
HenchpersonId PK int
Name varchar(200)
Description varchar(1024)
RatePerHour money
HenchersonStatusId int FK >- hs.HenchpersonStatusId 
SpecialityId int FK >- s.SpecialityId

Speciality as s
------------
SpecialityId PK int
Description varchar(1024)

HenchpersonStatus as hs
------------
HenchpersonStatusId PK int
Description varchar(64)

OrderStatus as os
----
OrderStatusID PK int
Name string

